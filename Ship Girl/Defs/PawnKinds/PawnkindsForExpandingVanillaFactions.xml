	<!-- Mid tier - Outlander Ship -->

	<PawnKindDef ParentName="FSG_ShipGirlOutlanderBase">
		<defName>ShipgirlOutlander</defName>
		<label>humanoid warship</label>
		<race>ShipGirlRace</race>
		<combatPower>150</combatPower>
		<maxGenerationAge>40</maxGenerationAge>
		<gearHealthRange>0.7~2.3</gearHealthRange>
		<apparelMoney>400~1000</apparelMoney>
		<apparelTags>
			<li>IndustrialBasic</li>
			<li>IndustrialAdvanced</li>
			<li>IndustrialMilitaryBasic</li>
			<li>IndustrialMilitaryAdvanced</li>
		</apparelTags>
		<apparelAllowHeadgearChance>0.7</apparelAllowHeadgearChance>
		<weaponMoney>250~345</weaponMoney>
		<weaponTags>
			<li>Gun</li>
		</weaponTags>
		<techHediffsChance>0</techHediffsChance>
		<techHediffsMoney>50~20000</techHediffsMoney>
		<techHediffsTags>
		  
			<li>Simple</li>
			<li>ImplantEmpireCommon</li>
		</techHediffsTags>
		<backstoryFiltersOverride>
				<li>
					<categories>
						<li>OutlanderShipGirlBackstories</li>
					</categories>
				</li>
		</backstoryFiltersOverride>
		<inventoryOptions>
			<skipChance>0.9</skipChance>
			<subOptionsChooseOne>
				<li>
				<thingDef>MedicineIndustrial</thingDef>
				<countRange>1</countRange>
				</li>
			</subOptionsChooseOne>
		</inventoryOptions>
		<biocodeWeaponChance>0.3</biocodeWeaponChance>
	</PawnKindDef>
	


