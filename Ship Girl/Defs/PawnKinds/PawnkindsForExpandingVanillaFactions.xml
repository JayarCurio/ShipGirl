	<PawnKindDef Name="FSG_ShipGirlPirateBase" ParentName="PirateScavTierBase">
		<defName>ShipgirlPirateGun</defName>
		<race>ShipGirlRace</race>
		<label>pirate gunship</label>
		<combatPower>250</combatPower>
		<apparelMoney>300~500</apparelMoney>
		<weaponMoney>200~300</weaponMoney>
		<weaponTags>
			<li>Gun</li>
		</weaponTags>
		<backstoryFiltersOverride>
			<li>
