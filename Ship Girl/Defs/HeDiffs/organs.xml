<!--Engine Upgrades-->

  

  <!-- Nuclear Engine -->

	<HediffDef ParentName="AddedBodyPartBase">
		<defName>FSG_NuclearEngine</defName>
		<label>nuclear ship engine</label>
		<labelNoun>a nuclear ship engine</labelNoun>
		<description>An installed nuclear ship engine.</description>
    
		<spawnThingOnRemoved>FSG_NuclearEngine</spawnThingOnRemoved>
		
		<hediffGivers>
			<li Class="HediffGiver_Random">
			<mtbDays>600</mtbDays>
			<hediff>Carcinoma</hediff>
			<partsToAffect>
				<li>Torso</li>
			</partsToAffect>
			</li>
		</hediffGivers>
		<stages>
			<li>
				<hungerRateFactor>0.25</hungerRateFactor>
				<foodPoisoningChanceFactor>0</foodPoisoningChanceFactor>
			</li>
		</stages>
		<comps>
			<li Class="HediffCompProperties_ReactOnDamage">
				<damageDefIncoming>EMP</damageDefIncoming>
				<vomit>true</vomit>
			</li>
		</comps>
		<addedPartProps>
			<solid>false</solid>
			
			
			<partEfficiency>1.1</partEfficiency>
			<betterThanNatural>true</betterThanNatural>			
		</addedPartProps>
	</HediffDef>

	<ThingDef ParentName="FSG_FabricatedBodypartBase">
		<defName>FSG_NuclearEngine</defName>
		<label>nuclear ship engine</label>
		<description>A full size nuclear engine, designed to be installed in the engine room of a nuclear age battleship, and miniaturized via ship girl psyence. Becasue of it's size, it is much better shielded than a nuclear stomach.</description>
		<graphicData>
				<texPath>Organs/Engines/BoilerNuke</texPath>
				<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<thingSetMakerTags><li>RewardStandardLowFreq</li></thingSetMakerTags>
		<costList>
			<Plasteel>15</Plasteel>
			<Uranium>15</Uranium>
			<ComponentSpacer>4</ComponentSpacer>
		</costList>
		<recipeMaker>
			<!--<researchPrerequisite>ArtificialMetabolism</researchPrerequisite>-->
		</recipeMaker>
		<statBases>
			<MarketValue>400</MarketValue>
		</statBases>
	</ThingDef>

	<RecipeDef ParentName="SurgeryInstallBodyPartArtificialBase">
		<defName>InstallFSG_Nuclearengine</defName>
		<label>install nuclear engine</label>
		<description>Install a nuclear engine.</description>

		<workerClass>Recipe_InstallArtificialBodyPart</workerClass>
		<jobString>Installing nuclear engine.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>FSG_NuclearEngine</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>FSG_NuclearEngine</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Engine</li>
		</appliedOnFixedBodyParts>
		<addsHediff>FSG_NuclearEngine</addsHediff>
	</RecipeDef>
	
