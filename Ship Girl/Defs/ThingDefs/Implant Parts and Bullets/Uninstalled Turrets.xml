			<texPath>Turrets/Smithed</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<thingSetMakerTags><li>RewardStandardLowFreq</li></thingSetMakerTags>
		<costList>
		  <Steel>100</Steel>
		 
		</costList>
		<recipeMaker>
		  <researchPrerequisite>Smithing</researchPrerequisite>
		</recipeMaker>
	</ThingDef>
