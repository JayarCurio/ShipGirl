	<!--Explosive -->

	
	<HediffDef ParentName="AddedBodyPartBase">
		<defName>FSG_InstalledTurretPetard</defName>
		<label>installed cast explosive cannon(PCF)</label>
		<labelNoun>a cast cannon, with explosive rounds</labelNoun>
		<description>An installed cannon, made of build up layers of shrink fit pig iron, loaded with explosive shells./n/n/n
While lacking the range, speed, and blast radius of modern cannons, these archaic tools will help against slow moving, or tightly clusted foes.</description>
    	<spawnThingOnRemoved>FSG_PartTurretPetard</spawnThingOnRemoved>
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>1.0</partEfficiency>
			<betterThanNatural>true</betterThanNatural>
		</addedPartProps>
		<hediffGivers>
		</hediffGivers>
		<stages>
			<li>
			<capMods>
				<li>
					<capacity>Moving</capacity>
					<postFactor>0.90</postFactor>
				</li>
			</capMods>
			</li>
		</stages>
		<comps>
		<li Class="OrenoPCF.HediffCompProperties_VerbGiverExtended">
			<toggleLabel>Toggle Explosive Cannon</toggleLabel>
			<toggleDescription>Disable the cannon</toggleDescription>
			<!-- <toggleBackgroundPath>UI/Widgets/Prosthetic/DesButBGLG</toggleBackgroundPath> -->
			<toggleIconPath>Turrets/Smithed</toggleIconPath>
			<toggleIconAngle>-32.0</toggleIconAngle>
			<verbs>
				<li>
					<label>Fire Explosive Cannon</label>
					<verbClass>Verb_Shoot</verbClass>
					<accuracyTouch>0.50</accuracyTouch>
					<accuracyShort>0.60</accuracyShort>
					<accuracyMedium>0.55</accuracyMedium>
					<accuracyLong>0.50</accuracyLong>
					<forcedMissRadius>2.9</forcedMissRadius>
					<hasStandardCommand>true</hasStandardCommand>
					<defaultProjectile>FSG_Proj_Petard</defaultProjectile>
					<warmupTime>21</warmupTime>
					<range>27.9</range>
					<ticksBetweenBurstShots>30</ticksBetweenBurstShots>
					<burstShotCount>1</burstShotCount>
					<soundCast>Shot_IncendiaryLauncher</soundCast>
					<soundCastTail>GunTail_Heavy</soundCastTail>
					<muzzleFlashScale>10</muzzleFlashScale>
				</li>
			</verbs>
			<verbsProperties>
				<li>
					<label>Fire Explosive Cannon</label>
					<description>Artillery Weapons: A weapon mounted to your pawn that launches an explosive.</description>
					<uiIconPath>Turrets/Smithed</uiIconPath>
				</li>
			</verbsProperties>
		</li>
	</comps>
   </HediffDef>
			

  
  <RecipeDef ParentName="SurgeryInstallBodyPartArtificialBase">
